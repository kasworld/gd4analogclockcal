; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="gd4analogclockcal"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/image="res://analog_clock.png"
config/icon="res://analog_clock.png"

[autoload]

Global2d="*res://global/global2d.gd"
Config="*res://global/config.gd"

[display]

window/size/viewport_width=3840
window/size/viewport_height=2160
window/size/mode=3
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray()

[rendering]

textures/vram_compression/import_etc2_astc=true
